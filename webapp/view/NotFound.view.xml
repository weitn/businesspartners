<mvc:View
   controllerName="ns.businesspartners.controller.NotFound"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
   <Page id="notFoundPage" title="{i18n>NotFoundTitle}" showNavButton="true" navButtonPress="onNavBack">
        <content>
            <Button id="displaySuppliersBtn" text="{i18n>displaySuppliers}" press=".onDisplaySuppliers" class="sapUiTinyMarginEnd"/>
            <IllustratedMessage
                id="IllMsg"
                title="{i18n>NotFound}"
                description="{i18n>NotFound.description}"
                />
            <MessageView id="MsgView"/>
        </content>
    </Page>
</mvc:View>